{
    "name": "nowmovesoft/yii-filepond-assets",
    "description": "Yii2 assets bundles for FilePond",
    "keywords": [
        "yii2",
        "filepond",
        "asset",
        "bundle",
        "connection"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors":[
        {
            "name":"Michael Naumov",
            "email":"vommuan@gmail.com",
            "homepage":"http://mnaumov.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nowmovesoft/yii-filepond-assets/issues?state=open",
        "wiki": "https://github.com/nowmovesoft/yii-filepond-assets/wiki",
        "source": "https://github.com/nowmovesoft/yii-filepond-assets"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.14",
    },
    "require-dev": {
        "codeception/base": "^3.0",
        "codeception/verify": "^1.0",
        "codeception/specify": "^1.0"
    },
    "autoload":{
        "psr-4":{
            "nms\\filepond\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
